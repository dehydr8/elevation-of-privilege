{
  "name": "eop",
  "version": "0.20.0",
  "private": true,
  "main": "src/client/index.tsx",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@koa/cors": "^3.4.3",
    "basic-auth": "^2.0.1",
    "boardgame.io": "^0.50.2",
    "bootstrap": "^4.6.0",
    "cornucopia-cards-modified": "file:cornucopiaCards",
    "esm": "^3.2.25",
    "jointjs": "^3.7.4",
    "koa": "^2.14.2",
    "koa-body": "^4.2.0",
    "koa-router": "^12.0.0",
    "koa-send": "^5.0.1",
    "lodash": "^4.17.21",
    "node-persist": "^3.1.3",
    "react": "^17.0.2",
    "react-countdown-circle-timer": "^3.2.1",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-map-interaction": "^2.1.0",
    "react-nl2br": "^1.0.4",
    "react-router-dom": "^5.3.1",
    "react-scripts": "^5.0.1",
    "reactstrap": "^8.10.1",
    "reactstrap-confirm": "^1.3.2",
    "superagent": "^8.0.9",
    "uuid": "^9.0.0"
  },
  "type": "module",
  "scripts": {
    "start": "react-scripts start",
    "build": "run-p build:client build:server",
    "build:client": "react-scripts build",
    "build:server": "tsc -p tsconfig.server.json",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server": "node --unhandled-rejections=warn --es-module-specifier-resolution=node build-server/server/server.js",
    "preserver": "npm run build:server",
    "lint": "npm run eslint",
    "fixlint": "npm run eslint -- --fix",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx,.cjs .",
    "format": "npm run prettier -- --write",
    "checkformat": "npm run prettier -- --check",
    "prettier": "prettier \"./**/*.(js|jsx|ts|tsx|json|yml)\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^12.8.3",
    "@types/basic-auth": "^1.1.3",
    "@types/jest": "^27.5.2",
    "@types/koa__cors": "^3.3.0",
    "@types/koa-router": "^7.4.4",
    "@types/koa-send": "^4.1.3",
    "@types/lodash": "^4.14.189",
    "@types/node": "^16.11.26",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-helmet": "^6.1.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/seedrandom": "^3.0.5",
    "@types/superagent": "^4.1.15",
    "@types/uuid": "^9.0.2",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "nock": "^13.3.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "seedrandom": "^3.0.5",
    "supertest": "^6.3.3",
    "ts-toolbelt": "^9.6.0",
    "typescript": "^4.8.4"
  },
  "overrides": {
    "autoprefixer": "10.4.5"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!src/serviceWorker.js",
      "!src/components/board/board.ignoretest.js",
      "!src/jointjs/shapes.js",
      "!src/index.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 5,
        "functions": 5,
        "lines": 5,
        "statements": 5
      }
    },
    "coverageReporters": [
      "text",
      "lcov"
    ]
  }
}
