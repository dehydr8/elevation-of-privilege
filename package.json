    "lint": "eslint --ext .js,.jsx,.ts,.tsx,.cjs .",
    "fixlint": "eslint --fix --ext .js,.jsx,.ts,.tsx,.cjs .",
    "format": "prettier --write \"./**/*.(js|jsx|ts|tsx|json|yml)\""
